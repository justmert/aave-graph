{
  "name": "aave-graphql-editor",
  "version": "1.0.0",
  "description": "Graphql Editor for AAVE",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "sh -c 'yarn run graphclient build --dir $PWD/${0}'",
    "build-prod": "sh -c 'yarn run graphclient build --dir $PWD/${0} && yarn run graphclient serve-prod --dir $PWD/${0}'",
    "serve-prod": "sh -c 'yarn run graphclient serve-prod --dir $PWD/${0}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justmert/aave-graphql-editor"
  },
  "keywords": [
    "AAVE",
    "TheGraph",
    "GraphQL"
  ],
  "author": "Mert Köklü",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justmert/aave-graphql-editor/issues"
  },
  "homepage": "https://github.com/justmert/aave-graphql-editor#readme",
  "dependencies": {
    "@graphprotocol/client-cli": "^2.2.15",
    "cli-color": "^2.0.3",
    "figlet": "^1.5.2",
    "graphql": "^16.6.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0"
  }
}
